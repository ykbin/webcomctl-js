{
  "name": "webcomctl-js",
  "version": "0.0.5",
  "description": "Common controls library for the Web",
  "type": "module",
  "main": "src/index.mjs",
  "exports": {
    ".": "./src/index.mjs",
    "./document/*": "./src/document/*/index.mjs",
    "./document/*/template": "./src/document/*/template.mjs",
    "./control/*": "./src/control/*/index.mjs",
    "./control/*/template": "./src/control/*/template.mjs"
  },
  "scripts": {
    "build-node-debug": "webpack --config webpack.node.config.mjs --mode=development",
    "build-node-release": "webpack --config webpack.node.config.mjs --mode=production",
    "build-web-debug": "webpack --config webpack.config.mjs --mode=development",
    "build-web-release": "webpack --config webpack.config.mjs --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykbin/webcomctl-js.git"
  },
  "keywords": [
    "web", "node"
  ],
  "author": "Yurii Yakubin",
  "bugs": {
    "url": "https://github.com/ykbin/webcomctl-js/issues"
  },
  "homepage": "https://github.com/ykbin/webcomctl-js#readme",
  "dependencies": {
    "webnetq-js": "github:ykbin/webnetq-js"
  },
  "devDependencies": {
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "svgo": "^3.3.2"
  }
}
